{
	"name": "webgpgpu.ts",
	"version": "0.0.2",
	"scripts": {
		"test": "mocha",
		"test:build": "rollup -c rollup.tests.config.js",
		"test:karma": "karma start ./karma.debug.conf.js --browsers ChromeGPU",
		"test:watch": "mocha watch",
		"build": "rollup -c",
		"watch": "rollup -c -w",
		"biome": "biome check --write",
		"lint": "biome check",
		"prepack": "npm run lint && npm run build",
		"pack:list": "npm pack --dry-run",
		"benchmark": "tsx tests/benchmark.ts"
	},
	"keywords": [
		"webgpgpu",
		"gpu",
		"gpgpu",
		"webgpu",
		"typescript"
	],
	"main": "./lib/server.ts",
	"types": "./lib/server.d.ts",
	"exports": {
		"./biome": "./biome.json",
		".": {
			"browser": {
				"types": "./lib/client.d.ts",
				"import": "./lib/client.js",
				"require": "./lib/client.cjs"
			},
			"node": {
				"types": "./lib/server.d.ts",
				"import": "./lib/server.js",
				"require": "./lib/server.cjs"
			}
		},
		"./src": {
			"browser": "./src/client/index.ts",
			"node": "./src/server/index.ts"
		}
	},
	"imports": {
		"#webgpgpu": {
			"browser": "./src/client",
			"node": "./src/server",
			"default": "./src"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eddow/webgpgpu.git"
	},
	"description": "WebGPU computer-shader typed automation",
	"type": "module",
	"author": "Fran√ßois Marie <eeddow@gmail.com>",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@rollup/plugin-typescript": "^12.1.2",
		"@types/karma-chai": "^0.1.7",
		"@webgpu/types": "^0.1.54",
		"c8": "^10.1.3",
		"cross-env": "^7.0.3",
		"glob": "^11.0.1",
		"jsdom-global": "^3.0.2",
		"karma-esbuild": "^2.3.0",
		"karma-firefox-launcher": "^2.1.3",
		"rollup-plugin-dts": "^5.0.0",
		"rollup-plugin-multi-input": "^1.5.0",
		"sinon": "^19.0.2",
		"stream-browserify": "^3.0.0",
		"tsx": "^4.19.3",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@commitlint/prompt-cli": "^17.3.0",
        "@rollup/plugin-commonjs": "23.0.4",
        "@rollup/plugin-json": "5.0.2",
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-strip": "3.0.1",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.13",
        "@types/websocket": "1.0.5",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "chai": "^4.3.7",
        "codecov": "3.8.3",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "fs-extra": "11.1.0",
        "husky": "8.0.2",
        "karma": "6.4.1",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-mocha": "^2.0.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-rollup-preprocessor": "7.0.8",
        "karma-sourcemap-loader": "0.3.8",
        "lint-staged": "13.1.0",
        "mocha": "^10.2.0",
        "phantomjs-polyfill-object-assign": "0.0.2",
        "prettier": "2.8.1",
        "rollup": "3.7.3",
        "rollup-plugin-istanbul": "4.0.0",
        "rollup-plugin-node-builtins": "2.1.2",
        "rollup-plugin-typescript2": "0.34.1",
        "ts-node": "^10.9.1",
        "tslib": "2.4.1",
        "typedoc": "0.23.22",
        "typescript": "4.9.4"
	},
	"dependencies": {
		"@petamoriken/float16": "^3.9.1",
		"webgpu": "^0.2.11"
	}
}
